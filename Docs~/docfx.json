{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Runtime/**/*.cs",
            "Editor/**/*.cs"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "noRestore": true,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "getting-started/**.md",
          "getting-started/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "3lk3 Unity.Entities.Utility"
    },
    "fileMetadata": {
      "redirect_url": {
        "index.md": "getting-started/index.html"
      }
    },
    "template": [
      "default"
    ],
    "keepFileLink": false,
    "disableGitFeatures": true,
    "xref": [
      "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
    ]
  }
}